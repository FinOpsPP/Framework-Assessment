# {{ profile }} Maturity Assessment Framework

<table style="table-layout: fixed;">
    <tr>
        <th>Domain</th>
        <th>Capability &rarr; Clear, Actionable Steps</th>
    </tr>
{%- for domain in domains %}
    <tr>
        <td style="width: 50px; overflow-wrap: break-word; white-space: normal;">
        <b>{% if domain.serial_number -%}
        <a href="/components/domains/{{ domain.serial_number }}.md">{{ domain.serial_number }}</a>: {% endif -%}
        {{ domain.domain }}</b>
        </td>
        <td>
        <table style="table-layout: fixed;">
        {%- for capability in domain.capabilities %}
            <tr>
                <td style="width: 50px; overflow-wrap: break-word; white-space: normal;">
                {% if capability.serial_number -%}
                <a href="/components/capabilities/{{ capability.serial_number }}.md">{{ capability.serial_number }}</a>: {% endif -%}
                {{ capability.capability }}
                </td>
                <td>
                <table style="table-layout: fixed;">
                {%- for action in capability.actions %}
                    <tr>
                        <td style="width: 100px; overflow-wrap: break-word; white-space: normal;">
                        <a href="/components/actions/{{ action.serial_number }}.md">{{ action.serial_number }}</a>: {{ action.action }}
                        </td>
                    </tr>
                {%- endfor %}
                </table>
                </td>
            </tr>
        {%- endfor %}
        </table>
        </td>
    </tr>
{%- endfor %}
</table>